%{ 
	#include <stdio.h>
	#include "y.tab.h"
%} 

%% 

[0-9]+	{	yylval.n = (struct node*)malloc(sizeof(struct node));
		yylval.n->integer=atoi(yytext); 
		yylval.n->right=NULL;
		yylval.n->left=NULL;
		return DIGIT;}
"stop"	{	return STOP;}	
"write"	{	printf("write");return WRITE;}
"read"	{	return READ;}
[a-z]+	{	yylval.n = (struct node*)malloc(sizeof(struct node));
		yylval.n->character=yytext[1]; 
		yylval.n->right=NULL;
		yylval.n->left=NULL;
		return ID;}	
\=	{	return *yytext;}
\+	{	return *yytext;}
\-	{	return *yytext;}
\*	{	return *yytext;}
\/	{	return *yytext;}
\(	return *yytext;
\)	return *yytext;
[\n]	return *yytext;
%%

